<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="home_Navigation">
        <attr name="home_FontSize" format="reference|dimension"/>
        <attr name="home_EnlargeRate" format="reference|float"/>
        <attr name="home_FontColorNormal" format="reference|color"/>
        <attr name="home_FontColorSelect" format="reference|color"/>
        <attr name="home_FontColorLight" format="reference|color"/>
        <attr name="home_DefaultPos" format="reference|integer"/>
        <attr name="home_OrderMode" format="reference|string"/>
        <attr name="home_ItemSpace" format="reference|dimension"/>
    </declare-styleable>


        <declare-styleable name="RoundProgressBar">
            <attr name="roundColor" format="color"/>
            <attr name="roundProgressColor" format="color"/>
            <attr name="roundWidth" format="dimension"></attr>
            <attr name="textColor" format="color" />
            <attr name="textSize" format="dimension" />
            <attr name="max" format="integer"></attr>
            <attr name="textIsDisplayable" format="boolean"></attr>
            <attr name="style">
                <enum name="STROKE" value="0"></enum>
                <enum name="FILL" value="1"></enum>
            </attr>
        </declare-styleable>

</resources>